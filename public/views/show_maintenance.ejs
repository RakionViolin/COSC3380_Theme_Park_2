                    <th>Date Completed</th>
                </thead>
                <tbody>
                    <% for(let i =0;i < rows.length; i++){ %>
                        <tr>
                            <td><%= rows[i].Maintenance_ID %></td>
                            <td><%= rows[i].Name %></td>
                            <td><%= rows[i].Rides_coaster_ID %></td>
                            <td><%= rows[i].Date_Started %></td>
                            <td><%= rows[i].Date_Completed %></td>
                            </tr>
                    <% } %>                      
                </tbody>
            </table>  
